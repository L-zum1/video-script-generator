{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": "",
  "installCommand": "pip install -r requirements.txt",
  "framework": "python",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "index.py"
    },
    {
      "source": "/(.*)",
      "destination": "index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
